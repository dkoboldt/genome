#!/usr/bin/env genome-perl

BEGIN { 
    $ENV{UR_DBI_NO_COMMIT} = 1;
    $ENV{UR_USE_DUMMY_AUTOGENERATED_IDS} = 1;
}

use strict;
use warnings;

use above "Genome";
use Test::More;

use_ok("Genome::Test::Factory::ProcessingProfile");

my %provided;
my $name = Genome::Test::Factory::ProcessingProfile->create_name;
is($name, "test_processing_profile_1", "Created default name correctly");

my $filled_in = Genome::Test::Factory::ProcessingProfile->fill_in_missing_params(\%provided);
is($filled_in->{name}, "test_processing_profile_2", "Filled in a default name");

done_testing;
