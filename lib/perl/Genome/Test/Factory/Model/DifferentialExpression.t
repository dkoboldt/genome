#!/usr/bin/env genome-perl

BEGIN { 
    $ENV{UR_DBI_NO_COMMIT} = 1;
    $ENV{UR_USE_DUMMY_AUTOGENERATED_IDS} = 1;
}

use strict;
use warnings;

use above "Genome";
use Test::More;

use_ok("Genome::Test::Factory::Model::DifferentialExpression");
use_ok("Genome::Test::Factory::Model::RnaSeq");

my $input_patient = Genome::Individual->create(name => "FAKE_INDIVIDUAL");
my $input_subject = Genome::Sample->create(name => "FAKE_SAMPLE", source => $input_patient);
my $m = Genome::Test::Factory::Model::DifferentialExpression->setup_differential_expression_model();
ok($m->isa("Genome::Model::DifferentialExpression"), "Generated a de model");

done_testing;

